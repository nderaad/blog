<% include partials/header %>
<div class="ui container main">
  <h1>Blogs Page</h1>
  <% blogs.forEach(function(blog){ %>
      <h2><%= blog.title%></h2>
      <img src="<%=blog.image%>">
      <span> <%=blog.created%></span>
      <p><%= blog.body %></p>
      <a href="/blogs/<%=blog._id%>">Read More</a>
  <% }) %>
</div>

<% include partials/footer %>
